${element} <span style="visibility: hidden"><g:remoteLink action="moveUp" update="${type+'2'}" id="${element.encodeAsHTML()}" params="[setupInstance: setupInstance.id, type: type]"><img src="${g.resource(dir: 'images/icons', file: 'arrow_up.png')}" alt="${message(code:'up')}" align="top"/></g:remoteLink><g:remoteLink action="moveDown" update="${type+'2'}" id="${element.encodeAsHTML()}" params="[setupInstance: setupInstance.id, type: type]"><img src="${g.resource(dir: 'images/icons', file: 'arrow_down.png')}" alt="${message(code:'down')}" align="top"/></g:remoteLink><g:remoteLink action="editElement" update="${type + 'element' + i}" id="${element}" params="[setupInstance: setupInstance.id, type: type, i: i]"><img src="${resource(dir: 'images/icons', file: 'icon_edit.png')}" alt="${message(code: 'edit')}" align="top"/></g:remoteLink><g:remoteLink action="removeElement" update="${type+'2'}" id="${element}" params="[setupInstance: setupInstance.id, type: type]" before="if(!confirm('${message(code:'delete.warn')}')) return false"><img src="${g.resource(dir: 'images/icons', file: 'icon_remove.png')}" alt="${message(code:'delete')}" align="top"/></g:remoteLink></span>